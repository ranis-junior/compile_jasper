	<query language="sql"><![CDATA[SELECT PS.NAME                              AS PSY_NAME,
       PT.NAME                              AS PATIENT_NAME,
       TO_CHAR(PT.BIRTH_DATE, 'DD/MM/YYYY') AS BIRTH_DATE,
       (
           EXTRACT(
                   YEAR
                   FROM
                   AGE(CURRENT_DATE, PT.BIRTH_DATE)
           ) * 12 + EXTRACT(
                   MONTH
                   FROM
                   AGE(CURRENT_DATE, PT.BIRTH_DATE)
                    )
           )                                AS PROTOCOL_AGE,
       EXTRACT(
               YEAR
               FROM
               AGE(CURRENT_DATE, PT.BIRTH_DATE)
       )                                    AS PATIENT_AGE,
       ID.NAME                              AS DOMAIN_NAME,
       IV.STANDARD_SCORE,
       IV.RAW_SCORE
FROM PSYCHOLOGISTS PS
         INNER JOIN PATIENTS PT ON PT.ID_PSYCHOLOGIST = PS.ID
         INNER JOIN IDADI ON PT.ID = IDADI.ID_PATIENT
         INNER JOIN IDADI_VALUES IV ON IV.ID_IDADI = IDADI.ID
         INNER JOIN IDADI_DOMAINS AS ID ON ID.ID = IV.ID_DOMAIN
WHERE PT.ID = 1]]></query>
