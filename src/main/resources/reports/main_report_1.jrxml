	<query language="SQL"><![CDATA[SELECT
	PS.NAME AS PSY_NAME,
	PS.CRP,
	PT.NAME AS PATIENT_NAME,
	PT.GENDER AS PATIENT_GENDER,
	PT.MOTHER,
	PT.MOTHER_PROFESSION,
	PT.FATHER,
	PT.FATHER_PROFESSION,
	TO_CHAR(PT.BIRTH_DATE, 'DD/MM/YYYY') AS PATIENT_BIRTH_DATE,
	PR.DEMAND_DESCRIPTION,
	PR.INSTRUMENTS_USED
FROM
	PSYCHOLOGISTS PS
	INNER JOIN PATIENTS PT ON PT.ID_PSYCHOLOGIST = PS.ID
	INNER JOIN PATIENT_RECORDS PR ON PR.ID_PATIENT = PT.ID
WHERE
	PT.ID = $P{id_patient}]]></query>
